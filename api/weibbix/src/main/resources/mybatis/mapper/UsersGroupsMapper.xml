<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunwei.weibbix.mapper.UsersGroupsMapper">
   <insert id="insertUserGroupsSQL">
       INSERT INTO users_groups(userId,groupId) VALUE(#{userId},#{groupId})
   </insert>
    <delete id="deleteUserGroupSQL">
        DELETE FROM users_groups WHERE userId = #{userId} AND groupId = #{groupId}
    </delete>
</mapper>