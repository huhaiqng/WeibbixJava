    <!--获取未分配实例-->
    <select id="getNotAllocatedInstanceSQL" resultType="com.yunwei.weibbix.entity.Instance">
        SELECT * FROM instance WHERE allocated = FALSE AND env = #{env} and type = #{type} ORDER BY name,ip
    </select>
