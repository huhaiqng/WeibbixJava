    <select id="getTomcatInstancesByClusterIdSQL" resultType="com.yunwei.weibbix.entity.TomcatInstance">
        SELECT * FROM tomcat_ins
          WHERE id IN (SELECT instanceId FROM instance_cluster WHERE clusterId = #{clusterId})
          ORDER BY name
    </select>

